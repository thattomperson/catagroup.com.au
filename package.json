{
  "name": "catagroup-websites",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "env:pull": "turbo run env:pull --parallel",
    "catagroup": "npm run dev -- --filter=catagroup",
    "kovapumps": "npm run dev -- --filter=kovapumps",
    "mastervent": "npm run dev -- --filter=mastervent"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2"
  }
}
